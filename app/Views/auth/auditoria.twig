{% extends "partials/layout_a.twig" %}
{% block content %}
    <h4 class="page-header">Lista de Accesos</h4>
    <div class="row">
        <table class="table table-hover table-bordered">
            <tr>
                <th>ID</th>
                <th>IP</th>
                <th>FECHA</th>
            </tr>
            {% for item in datos %}
                <tr>
                    <td style="text-align: center;">{{ item.id }}</td>
                    <td>{{ item.ip }}</td>
                    <td>{{ item.created_at }}</td>
                </tr>
            {% endfor %}
        </table>
    </div>
{% endblock %}